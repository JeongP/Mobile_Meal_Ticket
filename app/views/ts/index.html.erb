<p id="notice"><%= notice %></p>

<h1>Ts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ts.each do |t| %>
      <tr>
        <td><%= t.title %></td>
        <td><%= link_to 'Show', t %></td>
        <td><%= link_to 'Edit', edit_t_path(t) %></td>
        <td><%= link_to 'Destroy', t, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New T', new_t_path %>
